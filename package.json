{
  "name": "graphx",
  "version": "0.0.1",
  "main": "src/index.js",
  "author": "Nebil Jabari",
  "description": "Visualization tool for complex apps",
  "engines": {
    "node": ">=12.16.0",
    "npm": ">=6.14.0"
  },
  "scripts": {
    "action": "npm run validation",
    "dev": "nodemon --ignore dist",
    "debug": "nodemon --inspect",
    "lint": "eslint --max-warnings=3 .",
    "typescript:watch": "tsc -w",
    "test": "jest",
    "snyk": "snyk",
    "validation": "npm run lint && tsc && npm test"
  },
  "dependencies": {
    "config": "^3.3.1",
    "espree": "^6.2.1",
    "flat": "^5.0.0",
    "glob": "^7.1.6"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "eslint": "^6.8.0",
    "eslint-plugin-no-secrets": "^0.6.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "snyk": "^1.311.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validation"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "dist",
      "mock"
    ]
  },
  "keywords": [
    "graph",
    "visualization"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nebiljabari/graphx.git"
  },
  "bugs": {
    "url": "https://github.com/nebiljabari/graphx/issues"
  },
  "homepage": "https://github.com/nebiljabari/graphx#readme"
}
